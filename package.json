{
  "name": "CannabisApp",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "admin:dev": "parcel admin-src/index.html",
    "admin:build": "rimraf release/admin && mkdirp release/admin && parcel build admin-src/index.html -d release/admin",
    "site:build": "rimraf release/site && mkdirp release/site && copyup \"website-src/*.*\" \"website-src/**/*.*\" release/site",
    "admin:serve": "npm run admin:build && firebase serve --only hosting:admin",
    "site:serve": "npm run site:build && firebase serve --only hosting:site",
    "admin:deploy": "npm run admin:build && firebase deploy --only hosting:admin",
    "site:deploy": "npm run site:build && firebase deploy --only hosting:site",
    "functions:deploy": "cd functions && firebase deploy --only functions",
    "all:deploy": "npm run admin:deploy && npm run site:deploy",
    "generate:apk": "cd android && ./gradlew assembleRelease && mv ./app/build/outputs/apk/release/app-release.apk ../release/DrCannabis-$(date +%Y%m%d%H%M).apk",
    "generate:aab": "cd android && ./gradlew bundleRelease && mv ./app/build/outputs/bundle/release/app.aab ../release/DrCannabis-$(date +%Y%m%d%H%M).aab",
    "devtools": "react-devtools",
    "clear": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force && npm install"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.5.0",
    "@react-native-community/netinfo": "^5.3.3",
    "@react-native-firebase/analytics": "6.0.3",
    "@react-native-firebase/app": "6.0.3",
    "@react-native-firebase/messaging": "6.0.3",
    "bootstrap": "^4.3.1",
    "classnames": "^2.2.6",
    "firebase": "^7.1.0",
    "firebase-admin": "^8.8.0",
    "firebaseui": "^4.3.0",
    "formik": "^1.5.8",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "pluralize": "^8.0.0",
    "postcss-modules": "^1.5.0",
    "prop-types": "^15.7.2",
    "react": "16.9.0",
    "react-bootstrap": "1.0.0-beta.14",
    "react-devtools": "^3.6.3",
    "react-dom": "^16.11.0",
    "react-firebaseui": "^4.0.0",
    "react-images-viewer": "^1.6.2",
    "react-native": "0.61.2",
    "react-native-animatable": "^1.3.3",
    "react-native-dotenv": "^0.2.0",
    "react-native-elements": "^1.2.7",
    "react-native-fast-image": "^7.0.2",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-google-signin": "^2.1.1",
    "react-native-image-crop-picker": "^0.26.1",
    "react-native-image-picker": "^1.1.0",
    "react-native-push-notification": "^3.1.9",
    "react-native-reanimated": "^1.3.0",
    "react-native-responsive-screen": "^1.3.0",
    "react-native-shadow": "^1.2.2",
    "react-native-size-matters": "^0.2.1",
    "react-native-snap-carousel": "^3.8.4",
    "react-native-svg": "^9.13.6",
    "react-native-swipe-list-view": "^2.1.3",
    "react-native-toast-with-button": "0.0.2",
    "react-native-vector-icons": "^6.6.0",
    "react-native-webview": "^8.0.2",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.9.4",
    "react-navigation-tabs": "^2.5.6",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-tabs": "^3.1.0",
    "standard": "^14.3.1",
    "uuid": "^3.3.3",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "7.6.2",
    "@babel/runtime": "7.6.2",
    "@react-native-community/eslint-config": "0.0.3",
    "babel-jest": "24.9.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-root-import": "^6.4.1",
    "copyfiles": "^2.1.1",
    "eslint": "6.5.1",
    "firebase-tools": "^7.12.1",
    "jest": "24.9.0",
    "metro-react-native-babel-preset": "0.51.1",
    "mkdirp": "^0.5.1",
    "parcel-bundler": "^1.12.4",
    "react-test-renderer": "16.9.0",
    "rimraf": "^3.0.0",
    "sass": "^1.25.0"
  },
  "jest": {
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!@ngrx|(?!deck.gl)|ng-dynamic)"
    ]
  },
  "browserslist": [
    "last 1 Chrome versions"
  ]
}
